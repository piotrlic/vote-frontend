{
  "private": true,
  "name": "food-track",
  "version": "0.0.0",
  "homepage": "https://piotrlic.github.io/food-track",
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--openssl-legacy-provider preact build --no-prerender",
    "serve": "sirv build --port 8080 --cors --single",
    "dev": "cross-env NODE_OPTIONS=--openssl-legacy-provider preact watch",
    "lint": "eslint src",
    "test": "jest",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "preact",
    "ignorePatterns": [
      "build/"
    ]
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-preact-pure": "^4.0.0",
    "eslint": "^8.30.0",
    "eslint-config-preact": "^1.3.0",
    "gh-pages": "^6.0.0",
    "jest": "^27.0.0",
    "jest-preset-preact": "^4.0.5",
    "preact-cli": "^3.4.2",
    "sirv-cli": "^2.0.2"
  },
  "dependencies": {
    "@babel/plugin-transform-react-jsx-source": "^7.22.5",
    "firebase": "^10.1.0",
    "preact": "^10.10.6",
    "preact-render-to-string": "^5.2.6",
    "preact-router": "^4.1.2",
    "typescript": "^5.1.6"
  },
  "jest": {
    "preset": "jest-preset-preact",
    "setupFiles": [
      "<rootDir>/tests/__mocks__/browserMocks.js",
      "<rootDir>/tests/__mocks__/setupTests.js"
    ]
  }
}
